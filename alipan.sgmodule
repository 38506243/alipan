#!name=阿里云盘 净化/解锁SVIP
#!desc=非一次性解锁，每次解锁需打开脚本,脚本作者：chxm1023
#!system=ios

[Script]
阿里云盘 = type=http-response,requires-body=1,timeout=10,pattern=^https?:\/\/(api|member)\.(aliyundrive|alipan)\.com\/(.+\/(users|activity|user\/get)|((business|apps)\/.+\/users|adrive\/.+\/user)),script-path=https://raw.githubusercontent.com/chxm1023/Rewrite/main/alidrive.js

[MITM]
hostname = %APPEND% *.aliyundrive.com, *.alipan.com
